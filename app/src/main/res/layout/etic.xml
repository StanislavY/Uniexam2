            android:text="123" />

        <TextView
            android:id="@+id/tOtvetsTic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/sOtvets" />

        <TextView
            android:id="@+id/tNOtvetsTic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
