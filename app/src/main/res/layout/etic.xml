            android:text="100" />

        <TextView
            android:id="@+id/tErrorsTic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/sErrors" />

        <TextView
            android:id="@+id/tNErrorsTic"
